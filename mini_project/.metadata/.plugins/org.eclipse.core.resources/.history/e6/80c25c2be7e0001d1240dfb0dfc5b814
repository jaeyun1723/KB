package com.air.dto;

public class SeatReservation {	// res_seat
	private int resId;	// res_id
	private String airId;	// air_id
	private String seatId;	// seat_id
	
	public SeatReservation(int resId, String airId, String seatId) {
		super();
		this.resId = resId;
		this.airId = airId;
		this.seatId = seatId;
	}
	
	
	
}
