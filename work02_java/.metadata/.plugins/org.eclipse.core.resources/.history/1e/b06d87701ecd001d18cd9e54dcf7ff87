package com.edu.test;

import java.util.ArrayList;
import java.util.List;

public class ArrayListTest3 {

	public static void main(String[] args) {
		
		List<String> list = new ArrayList<>();
		list.add("F");
		list.add("B");
		list.add("B");
		list.add("F");
		list.add("C");
		
		System.out.println(list);
		list.remove("B");
		System.out.println(list);
	}

}
